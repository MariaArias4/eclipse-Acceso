package com.taller.model;

public class Vehiculo {
    private int id;
    private String matricula;
    private String marca;
    private String modelo;
    private int anio;
    private int idPropietario; 
    
    // Para la vista extendida (JOIN), puedes agregar un objeto Propietario auxiliar
    private String nombrePropietarioAux;

    public Vehiculo(int id, String matricula, String marca, String modelo, int anio, int idPropietario,
			String nombrePropietarioAux) {
		super();
		this.id = id;
		this.matricula = matricula;
		this.marca = marca;
		this.modelo = modelo;
		this.anio = anio;
		this.idPropietario = idPropietario;
		this.nombrePropietarioAux = nombrePropietarioAux;
	}
    
	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getMatricula() {
		return matricula;
	}

	public void setMatricula(String matricula) {
		this.matricula = matricula;
	}

	public String getMarca() {
		return marca;
	}

	public void setMarca(String marca) {
		this.marca = marca;
	}

	public String getModelo() {
		return modelo;
	}

	public void setModelo(String modelo) {
		this.modelo = modelo;
	}

	public int getAnio() {
		return anio;
	}

	public void setAnio(int anio) {
		this.anio = anio;
	}

	public int getIdPropietario() {
		return idPropietario;
	}

	public void setIdPropietario(int idPropietario) {
		this.idPropietario = idPropietario;
	}

	public String getNombrePropietarioAux() {
		return nombrePropietarioAux;
	}

	public void setNombrePropietarioAux(String nombrePropietarioAux) {
		this.nombrePropietarioAux = nombrePropietarioAux;
	} 
}